    "build-production": "NODE_ENV=production node build.js",
    "deploy": "scp -r prod/ ubuntu@34.237.218.229:/home/ubuntu/devserver",
    "gzip": "gzip prod/**/* --extension=gz --extension=br",
    "move-jsurl": "cp prod/*.jsurl prod/_dev_/; rm prod/*.jsurl",
    "move-wasm": "cp prod/*.wasm prod/_dev_/; rm prod/*.wasm",
    "move-jsfile": "cp prod/*.jsfile prod/_dev_/; rm prod/*.jsfile",
    "move": "pnpm move-jsurl; pnpm move-wasm;",
    "predeploy": "pnpm build-production && pnpm move && pnpm gzip",
    "schema": "peechy --schema resolver/schema.kiwi --esm src/lib/lockfile.js --ts src/lib/lockfile.ts --go resolver/lockfile/lockfile.go"
